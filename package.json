{
  "name": "swan-cms",
  "version": "1.0.0",
  "description": "A CMS built with javascript",
  "main": "app/server/index.js",
  "scripts": {
    "build:development": "nf run -- webpack --colors --config ./_build/webpack/development.config.js",
    "build:production": "NODE_ENV=production webpack --colors --config ./_build/webpack/production.config.js",
    "lint": "eslint app config plugins webpack",
    "lint:fix": "eslint app config plugins webpack --fix",
    "knex": "nf run -- knex --knexfile _build/knexfile.js",
    "start": "node app/server",
    "test": "echo \"Error: no test specified\" && exit 1",
    "watch:client": "nf run -- webpack --colors --config ./_build/webpack/development.config.js --watch",
    "watch:server": "nf run -- nodemon --watch app --exec \"babel-node\" app/server",
    "watch": "npm-run-all -p watch:*"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jmdesiderio/swan-cms.git"
  },
  "keywords": [
    "CMS"
  ],
  "author": "Jason Desiderio",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jmdesiderio/swan-cms/issues"
  },
  "homepage": "https://github.com/jmdesiderio/swan-cms#readme",
  "dependencies": {
    "apollo-client": "^0.5.3",
    "babel-polyfill": "^6.16.0",
    "babel-preset-stage-0": "^6.16.0",
    "bcryptjs": "^2.3.0",
    "body-parser": "^1.15.2",
