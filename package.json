    "debug": "^2.3.3",
    "express": "^4.14.0",
    "graphql": "^0.7.2",
    "graphql-server-express": "^0.4.3",
    "graphql-tag": "^1.0.0",
    "graphql-tools": "^0.8.3",
    "immutable": "^3.8.1",
    "jsonwebtoken": "^7.1.9",
    "knex": "^0.12.6",
    "lodash": "^4.17.2",
    "moment": "^2.16.0",
    "normalize.css": "^5.0.0",
    "nunjucks": "^3.0.0",
    "objection": "^0.6.2",
